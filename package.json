{
  "name": "whatsapphold",
  "productName": "whatsapphold",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv public",
    "express": "nodemon dev/express.js"
  },
  "keywords": [],
  "author": {
    "name": "Christopher Seven Phiri",
    "email": "chris7phiri@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "whatsapphold"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "express-pouchdb": "^4.2.0",
    "fs-extra": "^9.1.0",
    "linkifyjs": "^3.0.0-beta.3",
    "multer": "^1.4.2",
    "pouchdb": "^7.2.2",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "sirv-cli": "^1.0.0",
    "smelte": "^1.1.5",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "svelte-file-dropzone": "^0.0.15",
    "svelte-icons": "^2.1.0",
    "svelte-spa-router": "^3.1.0",
    "svelte-video-player": "^1.2.5",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "video-metadata-thumbnails": "^1.0.22",
    "whatsapp-chat-parser": "^3.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "attractions": "^3.2.0",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "electron": "12.0.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.13",
    "postcss-load-config": "^3.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.32.12",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.7.2",
    "svelte-scrollto": "^0.2.0",
    "tailwindcss": "^2.0.3"
  }
}
